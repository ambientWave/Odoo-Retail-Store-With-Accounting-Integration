<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for menu.item -->

        <record id="seq_new_product" model="ir.sequence">
            <field name="name">مسلسل الصنف</field>
            <field name="code">new.product.sequence</field> <!-- unique -->
            <field name="prefix">I</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_new_retail_order" model="ir.sequence">
            <field name="name">مسلسل امر البيع</field>
            <field name="code">new.retail.order.sequence</field> <!-- unique -->
            <field name="prefix">O</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>